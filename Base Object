#include <iostream>
using namespace std;

class Base
{
    int _x,_y;
public:
    Base(){};
    Base(int x,int y)
    {
        Base b; // Base() {} -> object
        //Base b(x,y);//-> segmentation fault(recursive call)
        b._x = x;
        b._y = y;
        *this = b;// pointing to value 10 and 20
        b.print();
        
        
    }
    void print() { cout << _x << "\t" << _y << endl;}
};


int main()
{
    Base b(10,20);
    b.print();
    return 0;
}


